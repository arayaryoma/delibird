    "dependencies": {
        "connect-sse": "^1.2.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "eventsource": "^2.0.2",
        "express": "^4.18.2",
        "express-sslify": "^1.2.0",
        "helmet": "^6.0.1",
        "ioredis": "^5.3.1",
        "raven": "^2.6.4"
    },
    "devDependencies": {
        "@jest/globals": "^29.5.0",
        "@jest/types": "^29.5.0",
        "@swc-node/core": "^1.10.1",
        "@swc-node/register": "^1.6.2",
        "@swc/core": "^1.3.42",
        "@swc/jest": "^0.2.24",
        "@types/node": "^18.15.5",
        "concurrently": "^7.6.0",
        "eslint": "^8.36.0",
        "eslint-plugin-import": "^2.27.5",
        "glob-all": "^3.3.1",
        "jest": "^29.5.0",
        "nock": "^13.3.0",
        "nodemon": "^2.0.21",
        "prettier": "^2.8.6",
        "smee-client": "^1.2.3",
        "supertest": "^6.3.3",
        "typescript": "^5.0.2"
    },
    "engines": {
        "node": "19"
