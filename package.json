{
    "name": "smee.io",
    "private": true,
    "description": "Receives payloads then sends them to your locally running application.",
    "author": "Jason Etcovitch <jasonetco@gmail.com> (https://github.com/JasonEtco)",
    "license": "ISC",
    "repository": "github:probot/smee.io",
    "scripts": {
